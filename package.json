{
  "name": "@minko-fe/ts-template-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "a template for typescript project or plugin",
  "type": "module",
  "keywords": [
    "TODO"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/ts-template.git"
  },
  "author": "hemengke <https://github.com/hemengke1997>",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "commitlint": "commitlint -e",
    "up": "taze -I -w",
    "bump": "changeset",
    "version": "changeset version && changeset tag"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.4.4",
    "@minko-fe/commitlint-config": "^2.0.7",
    "@minko-fe/eslint-config": "^2.1.2",
    "@minko-fe/tsconfig": "^2.0.7",
    "bumpp": "^9.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.4",
    "tsup": "^6.7.0",
    "turbo": "^1.12.2",
    "typescript": "^5.3.3",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "commit-msg": "npm run commitlint",
    "pre-commit": "npm run lint"
  },
  "workspaces": [
    "packages/*"
  ]
}
