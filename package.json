{
  "name": "@minko-fe/ts-template-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.30.0",
  "description": "a template for typescript project or plugin",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/ts-template.git"
  },
  "keywords": [
    "TODO"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "build": "pnpm -r --parallel run build",
    "test": "pnpm -r --parallel run test",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "up": "taze -I -w -r",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "bump": "bumpp packages/*/package.json -c -p -t --all -x \"pnpm run changelog\""
  },
  "devDependencies": {
    "@minko-fe/eslint-config": "1.2.32",
    "@minko-fe/tsconfig": "^1.2.32",
    "bumpp": "^9.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "8.36.0",
    "pnpm": "^7.30.0",
    "taze": "^0.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  }
}
