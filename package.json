{
  "name": "@minko-fe/ts-template",
  "type": "module",
  "version": "1.0.0",
  "description": "a template for typescript project or plugin",
  "author": "hemengke <https://github.com/hemengke1997>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemengke1997/ts-template.git"
  },
  "keywords": [
    "TODO"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "pnpm run build && pnpm publish --access public",
    "preinstall": "npx only-allow pnpm",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "test": "vitest",
    "lint": "eslint .",
    "upgrade:deps": "pnpm update --i --L",
    "bump": "pnpm changelog && bumpp package.json CHANGELOG.md --commit --push --tag -all"
  },
  "devDependencies": {
    "@minko-fe/eslint-config": "1.2.15",
    "@minko-fe/tsconfig": "^1.2.15",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "8.34.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
}
